{
  "listeners": [
    {
      "address": {
        "socket_address": {
          "address": "0.0.0.0",
          "port_value": 6234
        }
      },
      "filter_chains": [
        {
          "filters": [
            {
              "name": "envoy.http_connection_manager",
              "config": {
                "codec_type": "http1",
                "stat_prefix": "dynamic_ingress_http",
                "route_config": {
                  "virtual_hosts": [
                    {
                      "name": "zomaland",
                      "domains": [
                        "zomaland.envoy.internal.zomans.com"
                      ],
                      "routes": [
                        {
                          "match": {
                            "prefix": ""
                          },
                          "route": {
                            "cluster": "zomaland-service"
                          }
                        }
                      ]
                    }
                  ]
                },
                "http_filters": [
                  {
                    "name": "envoy.router",
                    "config": {}
                  }
                ]
              }
            }
          ]
        }
      ]
    }
  ],
  "clusters": [
    {
      "name": "zomaland-service",
      "connect_timeout": "0.25s",
      "type": "STRICT_DNS",
      "lb_policy": "ROUND_ROBIN",
      "hosts": [
        {
          "socket_address": {
            "address": "$ZOMALAND_SERVICE",
            "port_value": 80
          }
        }
      ]
    }
  ],
  "cluster_template": {
    "type": "EDS",
    "connect_timeout": "1s",
    "lb_policy": "ROUND_ROBIN",
    "eds_cluster_config": {
      "eds_config": {
        "api_config_source": {
          "api_type": "GRPC",
          "grpc_services": [{
            "envoy_grpc": {
              "cluster_name": "tbn-xds"
            }
          }]
        }
      },
      "service_name": "ignored"
    },
    "max_requests_per_connection": 1000000,
    "http2_protocol_options": {},
    "hosts": [],
    "health_checks": [
      {
        "grpc_health_check": {},
        "timeout": "1s",
        "interval": "10s",
        "no_traffic_interval": "5s",
        "healthy_edge_interval": "3s",
        "unhealthy_edge_interval": "3s",
        "interval_jitter": "1s",
        "unhealthy_threshold": 3,
        "healthy_threshold": 3
      }
    ]
  }
}
